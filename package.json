	},
	"devDependencies": {
		"@swrlab/style-guide": "^1.0.3",
		"chai": "5.1.2",
		"dotenv": "^16.4.7",
		"eslint": "^9.17.0",
		"eslint-plugin-json": "^4.0.1",
		"mocha": "^11.0.1",
		"prettier": "^3.4.2"
	},
	"engines": {
		"node": ">=20"
	},
	"prettier": "@swrlab/style-guide/prettier"
